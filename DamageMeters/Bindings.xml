<Bindings>
	<Binding name="DAMAGEMETERS_TOGGLESHOW" description="Toggle Visible" header="DAMAGEMETERSHEADER">
		DamageMeters_ToggleShow();
	</Binding>
	<Binding name="DAMAGEMETERS_CYCLEQUANT" description="Cycle Visible Quantity">
		DamageMeters_CycleQuant(true, DamageMeters_flags[DMFLAG_applyFilterToManualCycle]);
	</Binding>
	<Binding name="DAMAGEMETERS_CYCLEQUANTBACK" description="Cycle Visible Quantity Backwards">
		DamageMeters_CycleQuantBack(true, DamageMeters_flags[DMFLAG_applyFilterToManualCycle]);
	</Binding>
	<Binding name="DAMAGEMETERS_CLEAR" description="Clear List">
		DamageMeters_Clear();
	</Binding>
	<Binding name="DAMAGEMETERS_SYNCCLEAR" description="Sync Clear List">
		DamageMeters_SyncClear();
	</Binding>
	<Binding name="DAMAGEMETERS_TOGGLEPAUSED" description="Toggle Paused">
		DamageMeters_TogglePause(true);
	</Binding>
	<Binding name="DAMAGEMETERS_SYNCPAUSE" description="Sync Pause">
		DamageMeters_SyncPause();
	</Binding>
	<Binding name="DAMAGEMETERS_SYNCUNPAUSE" description="Sync Unpause">
		DamageMeters_SyncUnpause();
	</Binding>
	<Binding name="DAMAGEMETERS_SYNCREADY" description="Send Sync Ready">
		DamageMeters_SyncReady(true);
	</Binding>
	<Binding name="DAMAGEMETERS_SHOWREPORTFRAME" description="Show Report Frame">
		DamageMeters_ShowReportFrame();
	</Binding>
	<Binding name="DAMAGEMETERS_SWAPMEMORY" description="Swap Memory">
		DamageMeters_Swap(false);
	</Binding>
	<Binding name="DAMAGEMETERS_TOGGLESHOWMAX" description="Toggle Show Max Bars">
		DamageMeters_ToggleMaxBars(true);
	</Binding>
	<Binding name="DAMAGEMETERS_TOGGLESHOWFIGHTASPS" description="Show Fight As PS">
		DamageMeters_ToggleShowFightAsPS();
	</Binding>
	<Binding name="DAMAGEMETERS_TOGGLENORMALANDFIGHT" description="Toggle Between Normal and Fight Quantities">
		DamageMeters_ToggleNormalAndFight();
	</Binding>
	<Binding name="DAMAGEMETERS_TOGGLEMINIMODE" description="Toggle Mini Mode">
		DamageMeters_ToggleMiniMode(true);
	</Binding>
</Bindings>