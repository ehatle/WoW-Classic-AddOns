			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				SmallMoneyFrame_OnLoad();
			</OnLoad>
			<OnEvent>
				MoneyFrame_OnEvent();
			</OnEvent>
			<OnShow>
				MoneyFrame_UpdateMoney();
			</OnShow>
			<OnHide>
				if ( this.hasPickup == 1 ) then
					CoinPickupFrame:Hide();
					this.hasPickup = 0;
				end
			</OnHide>
		</Scripts>
	</Frame>

